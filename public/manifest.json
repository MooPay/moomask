{
  "name": "MooMask",
  "description": "Binance Smart Chain Browser Extension Wallet",
  "author": "MooMask",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_title": "MooMask",
    "default_popup": "popup.html"
  }
}
